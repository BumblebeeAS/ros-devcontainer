{
	"name": "ROS devcontainer for vscode",
	"dockerComposeFile": [
		"../docker-compose.yml"
	],
	"service": "workspace",
	"workspaceFolder": "/workspace",
	"shutdownAction": "stopCompose",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-vscode.cpptools",
				"ms-python.python",
				"ms-iot.vscode-ros",
				"twxs.cmake",
				"redhat.vscode-xml"
			]
		}
	}
    //"appPort": [80],
}